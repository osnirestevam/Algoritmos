algoritmo "contar_ate_onde_o_usuario_quiser"

var
   contador, numero: Inteiro

inicio
      contador <- numero
      Escreva ("Digite um número: ")
      Leia (numero)
      
      Enquanto (contador <= numero) faca
      Escreva (contador)
      contador <- contador + 1
      FimEnquanto


fimalgoritmo
